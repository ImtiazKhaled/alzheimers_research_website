assword)\r\n      .catch(err => {\r\n        switch (err.code) {\r\n          case \"auth/invalid-email\":\r\n          case \"auth/user-disabled\":\r\n          case \"auth/user-not-found\":\r\n            setEmailError(err.message)\r\n            break\r\n          case \"auth/wrong-password\":\r\n            setPasswordError(err.message)\r\n            break\r\n          default:\r\n            break\r\n        }\r\n      })\r\n\r\n  }\r\n\r\n  function logout() {\r\n    fire.auth().signOut()\r\n    setUser(' ')\r\n  }\r\n\r\n  function authListener() {\r\n    fire.auth().onAuthStateChanged(user => {\r\n      if (user) {\r\n\r\n        setUser(user.uid)\r\n\r\n        if (!emailError && !passwordError) {\r\n          setLoggedIn(true)\r\n          clearForm()\r\n        }\r\n      }\r\n      else {\r\n        setLoggedIn(false)\r\n        setUser(' ')\r\n      }\r\n    })\r\n  }\r\n\r\n  React.useEffect(() => {\r\n    authListener()\r\n  }, [])\r\n\r\n\r\n  return (\r\n\r\n    <div className='contact-container'>\r\n      <Display2 marginBottom=\"scale1000\"> {t('adminPanel')} </Display2>\r\n      {\r\n        loggedIn === false && <form>\r\n          <FormControl\r\n            label={`${t(\"\")}`}\r\n            //error={ shouldShowError ? `${t(\"emailErreurMessage\")}` : null }\r\n            error={shouldShowError ? `${t(\"emailErreurMessage\")}` : emailError}\r\n          >\r\n            <Input\r\n              placeholder={`${t(\"email\")}`}\r\n              name=\"email\"\r\n              value={email}\r\n              onChange={onEmailChange}\r\n              onBlur={() => setIsVisited(true)}\r\n              error={shouldShowError}\r\n              overrides={shouldShowError ? { After: Negative } : {}}\r\n              type=\"email\"\r\n              required\r\n            />\r\n          </FormControl>\r\n\r\n          <FormControl label={`${t(\"\")}`}\r\n            error={passwordError}>\r\n            <Input\r\n              placeholder={`${t(\"password\")}`}\r\n              type='password'\r\n              name=\"password\"\r\n              value={password}\r\n              onChange={event => setPassword(event.currentTarget.value)}\r\n              required\r\n            />\r\n          </FormControl>\r\n\r\n          <Button onClick={event => {\r\n            event.preventDefault()\r\n            login(event)\r\n          }}>{t(\"logIn\")}</Button>\r\n\r\n        </form>\r\n      }\r\n\r\n      {loggedIn === true && <Button onClick={event => logout(event)} >{t(\"logOut\")} &nbsp; {username}</Button>}\r\n\r\n    </div>\r\n\r\n  )\r\n}\r\n\r\nexport default Form",["152","153"],"C:\\Users\\tisra\\Downloads\\School Projects\\alzheimers_research_website\\website\\src\\components\\admin.js",[],"C:\\Users\\tisra\\Downloads\\School Projects\\alzheimers_research_website\\website\\src\\components\\AddAdmin.js",[],"C:\\Users\\tisra\\Downloads\\School Projects\\alzheimers_research_website\\website\\src\\components\\dementia.js",["154","155","156","157","158"],"import React from \"react\";\r\nimport DementiaCard from \"./dementia_card\";\r\nimport { Display2 } from \"baseui/typography\";\r\nimport { FlexGrid, FlexGridItem } from \"baseui/flex-grid\";\r\n\r\nimport { useStyletron } from 'baseui';\r\nimport { Grid, Cell } from 'baseui/layout-grid';\r\nimport { t } from \"react-switch-lang\";\r\nimport { useDementia, useDementiaUpdate } from '../context/DementiaContext'\r\nimport { useUser } from '../context/UserContext'\r\nconst Dementia = () => {\r\n\r\n  const user = useUser();\r\n  const dementia = useDementia();\r\n  const updateDementia = useDementiaUpdate();\r\n\r\n  React.useEffect(() => {\r\n    updateDementia()\r\n  }, [])\r\n\r\n  return (\r\n    <div className=\"dementia-container\">\r\n      <Display2 marginBottom=\"scale1000\"> {t(\"dementiaInformation\")} </Display2>\r\n\r\n      <Grid >\r\n        {dementia.map((dementia) => (\r\n          <Cell span={[12, 8, 4]} key={dementia.id}>\r\n            <Inner><D