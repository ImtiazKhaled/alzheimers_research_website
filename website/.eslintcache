\n        var addressformatted = `https://www.google.com/maps/dir/0000,0000/`\r\n        resource.address.split(' ').forEach(word => addressformatted += word + '+')\r\n\r\n\r\n        // const description = `Phone number: ${numberformatted} <newline> Address: ${addressformatted} <newline> Website: ${websiteformatted}`\r\n        const payload = { lang, title: resource.name, phoneNumber: resource.number, url: websiteformatted, address: resource.address, gmaps: addressformatted, iframe: resource.iframe }\r\n\r\n        // console.log(payload)\r\n\r\n        db.collection(\"facilities\").add(payload)\r\n            .then(function () {\r\n                console.log(\"Document successfully written!\");\r\n            })\r\n            .catch(function (error) {\r\n                console.error(\"Error writing document: \", error);\r\n            });\r\n        changeResource()\r\n    }\r\n\r\n    return (\r\n        <ResourceContext.Provider value={resources}>\r\n            <ResourceUpdateContext.Provider value={changeResource}>\r\n                <ResourceAddContext.Provider value={addResource}>\r\n                    {children}\r\n                </ResourceAddContext.Provider>\r\n            </ResourceUpdateContext.Provider>\r\n        </ResourceContext.Provider>\r\n    )\r\n}\r\n","C:\\Users\\tisra\\Downloads\\School Projects\\alzheimers_research_website\\website\\src\\components\\Menu.js",[],"C:\\Users\\tisra\\Downloads\\School Projects\\alzheimers_research_website\\website\\src\\components\\NotFound.js",[],"C:\\Users\\tisra\\Downloads\\School Projects\\alzheimers_research_website\\website\\src\\App.js",[],"C:\\Users\\tisra\\Downloads\\School Projects\\alzheimers_research_website\\website\\src\\components\\Research.js",[],"C:\\Users\\tisra\\Downloads\\School Projects\\alzheimers_research_website\\website\\src\\components\\CommunityCard.js",["285","286","287","288"],"C:\\Users\\tisra\\Downloads\\School Projects\\alzheimers_research_website\\website\\src\\components\\