] = React.useState(\"\");\r\n    const [iframe, setIframe] = React.useState(\"\");\r\n\r\n\r\n\r\n    const handleEdit = () => {\r\n        setTitle(props.title);\r\n        setUrl(props.url);\r\n        setCountry(undefined);\r\n        setPhoneNumber(props.phoneNumber);\r\n        setAddress(props.address);\r\n        setGmaps(props.gmaps);\r\n        setIframe(props.iframe);\r\n        setLang(props.lang);\r\n        setEdit(true);\r\n    }\r\n\r\n\r\n    const saveEntry = () => {\r\n        db.collection('facilities').doc(props.id).set({\r\n            address: address ? address : \"\",\r\n            description: \"\",\r\n            gmaps: gmaps ? gmaps : \"\",\r\n            iframe: iframe ? iframe : \"\",\r\n            lang: lang ? lang : \"\",\r\n            phoneNumber: phoneNumber ? phoneNumber : \"\",\r\n            title: title ? title : \"\",\r\n            url: url ? url : \"\"\r\n\r\n        });\r\n\r\n        setEdit(false);\r\n    }\r\n\r\n    const deleteEntry = () => {\r\n        db.collection('facilities').doc(props.id).delete();\r\n\r\n        setEdit(false);\r\n    }\r\n\r\n\r\n\r\n    const displayMode = <FlexGrid\r\n        flexGridColumnCount={[1, 1, 1, 2]}\r\n        flexGridColumnGap=\"scale800\"\r\n        flexGridRowGap=\"scale800\"\r\n\r\n\r\n    >\r\n        <FlexGridItem > <H6>{props.title}</H6></FlexGridItem>\r\n\r\n        <FlexGridItem >\r\n            <p> {t(\"phoneNumber\")} :   &nbsp; <a href={tel}>{props.phoneNumber}</a></p>\r\n            <p>{t(\"address\")} : &nbsp;\r\n            <StyledLink href={addressToDisplay}>\r\n                    {props.address}\r\n                </StyledLink>\r\n            </p>\r\n\r\n\r\n            <p>{t(\"website\")} : &nbsp;\r\n            <StyledLink href={props.url}>\r\n                    {t(\"linkToWebsite\")}\r\n                </StyledLink>\r\n            </p>\r\n\r\n\r\n            <Button kind={KIND.minimal}\r\n                size={SIZE.large}\r\n                shape={SHAPE.pill} 